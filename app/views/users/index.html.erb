<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <%# <th colspan="3"></th> %>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.mobile_number %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



